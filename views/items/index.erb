<h3>Items index</h3>
<ul>
<% @items.each do |item| %>
	<li>
		<%= item.title %>
		<form action="/items/<%= item.id %>" method="POST">
			<input type="hidden" name="_method" value="DELETE">
			<input type="submit" value="Delete">
		</form>
		<a class='btn' href='/items/edit/<%= item.id %>'>Edit</a>
	</li>
<% end %>
</ul>